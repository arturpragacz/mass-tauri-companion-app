{
  "build": {
    "beforeDevCommand": "cd frontend && yarn dev",
    "beforeBuildCommand": "cd frontend/ && yarn && yarn build",
    "frontendDist": "../frontend/dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "macOS": {
      "signingIdentity": "955D523MK8"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
